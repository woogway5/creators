          "_customHTML": "<section class=\"form1\" group=\"Forms\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size:</header>\n        <input type=\"range\" inline title=\"Top:\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom:\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show title:</header>\n        <input type=\"checkbox\" title=\"Show title:\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show subtitle:\" name=\"showSubtitle\">\n        <select title=\"Text align:\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <header>Form:</header>\n        <input type=\"color\" title=\"Color border:\" name=\"colorBorderForm\" value=\"#c900fc\">\n        <!--  -->\n        <header>Background:</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background image\" value=\"../_images/background15.png\">\n            <input type=\"color\" title=\"Background color\" value=\"#ffffff\" selected> \n        </fieldset>\n        <header>Overlay:</header>\n        <input type=\"checkbox\" title=\"Overlay:\" name=\"overlay\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay color:\" name=\"overlayColor\" value=\"#7567f8\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay opacity:\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"bg.type!='color' && overlay\">\n    </mbr-parameters>\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container align-center mbr-white\">\n        <!-- Content -->\n        <div class=\"mbr-row mbr-jc-c flex-al-c\">\n            <!-- Title -->\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-5\" mbr-if=\"showTitle || showSubtitle\">\n                <div class=\"title-wrap\" mbr-if=\"showTitle || showSubtitle\">\n                    <h3 class=\"mbr-white mbr-text mbr-fonts-style mbr-extralight\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" mbr-class=\"{'align-left' : titleAlign == 1, 'align-center' : titleAlign == 2, 'align-right' : titleAlign == 3}\"><b>質問募集中！！</b></h3>\n                    <p class=\"mbr-white mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" mbr-class=\"{'align-left' : titleAlign == 1, 'align-center' : titleAlign == 2, 'align-right' : titleAlign == 3}\">Stay In Touch With Us My Friend!\n                    </p>\n                </div>\n            </div>\n            <!-- Form -->\n            <div mbr-form class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-7\">\n                <form class=\"mbr-form\" action=\"ladybugs.yanagawa@gmail.com\" method=\"post\" name=\"Mobirise Form\" target=\"_top\" novalidate>\n                    <div submit-success>\n                        <template data-form-alert type=\"amp-mustache\">お問い合わせありがとうございます！</template>\n                    </div>\n                    <div submit-error>\n                        <template data-form-alert type=\"amp-mustache\">お問い合わせありがとうございます！</template>\n                    </div>\n\n                    <div class=\"fieldset\">\n                        <label class=\"input-label\">\n                            <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Email\">\n                            <input type=\"email\" name=\"form[data][0][1]\" data-form-field=\"Email\" mbr-theme-style=\"display-4\" placeholder=\"お問い合わせ内容\" required>\n                            <!-- Button -->\n                            <div mbr-buttons class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-theme-style=\"display-4\" mbr-class=\"{'align-left' : alignTitle == 1, 'align-center' : alignTitle == 2, 'align-right' : alignTitle == 3}\"><a class=\"btn btn-danger\" type=\"submit\" data-app-placeholder=\"Type Text\" href=\"mailto:ladybugs.yanagawa@gmail.com\">送信ボタン</a></div>\n                        </label>\n                    </div>\n\n                </form>\n            </div>\n        </div>\n    </div>\n</section>",
